{
    "name": "haipham22/repository",
    "description": "Repository pattern for Laravel",
    "require": {
        "torann/laravel-repository": "^0.6.1"
    },
    "authors": [
        {
            "name": "Hải Phạm",
            "email": "haipham.net@gmail.com",
            "homepage": "https://haipham.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "haipham22\\Repository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "haipham22\\Repository\\Providers\\RepositoryServiceProvider"
            ],
            "dont-discover": [
                "torann/laravel-repository"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
